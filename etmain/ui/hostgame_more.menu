#include "ui/menudef.h"#include "ui/menumacros.h"// Defines //#define WINDOW_X		16#define WINDOW_Y		16#define WINDOW_WIDTH	608#define WINDOW_HEIGHT	456#define GROUP_NAME		"grpHostGameMore"// Host Game More Menu //menuDef {	name		"hostgame_more"	visible		0	fullscreen	0	rect		WINDOW_X WINDOW_Y WINDOW_WIDTH WINDOW_HEIGHT	style		WINDOW_STYLE_FILLED	onEsc {		close hostgame_more ;		open hostgame_advanced	}// Window //	WINDOW( "MORE", 50)// Subwindows //#define SUBWINDOW_WIDTH// More //	SUBWINDOW( 6, 32, (SUBWINDOW_WIDTH), 24, "MISC" )	MULTI( 8, 48, (SUBWINDOW_WIDTH)-4, 10, "Country Flags:", .2, 8, "g_countryflags", cvarFloatList { "No" 0 "Yes" 1 }, "Enable Country Flags on the server" )	// Buttons //		BUTTON( 6, WINDOW_HEIGHT-24, WINDOW_WIDTH-12, 18, "BACK", .3, 14, close hostgame_more ; open hostgame_advanced )}